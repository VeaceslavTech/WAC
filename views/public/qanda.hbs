<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>Q&A</title>
    <link rel="stylesheet" href="/css/Design.css">
</head>
<body>


<div class="QA-content">
    {{#each questions}}
        <div class="question-container">
            <div class="question">
                <h3>{{this.questionText}}</h3>
            </div>
            <div class="answer" style="display: none;">
                <p>{{this.answerText}}</p>
            </div>
        </div>
    {{/each}}
</div>

<script src="/scripts/qandaScript.js"></script>
<script src="/scripts/openLoginScript.js"></script>



</body>
</html>
